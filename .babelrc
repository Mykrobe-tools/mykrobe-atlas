{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": ["last 2 versions"]
				}
			}
		],
		"stage-0",
		"react"
	],
	"plugins": [
		"inline-dotenv",
		["transform-runtime", {
			"helpers": false,
			"polyfill": false,
			"regenerator": true
		}],
		"transform-flow-strip-types"
	],
	"env": {
		"production": {
			"presets": ["react-optimize"],
			"plugins": [
				"./babel-plugin-electron-module-resolver",
			]
		},
		"development": {
			"plugins": ["./babel-plugin-electron-module-resolver"]
		}
	}
}
