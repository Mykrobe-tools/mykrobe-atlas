{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": ["last 2 versions"]
				}
			}
		],
		"stage-0",
		"react"
	],
	"plugins": [
		"add-module-exports",
		"inline-dotenv",
		"transform-runtime",
		"transform-flow-strip-types"
	],
	"env": {
		"production": {
			"presets": ["react-optimize"],
			"plugins": ["./babel-plugin-electron-module-resolver"]
		},
		"development": {
			"plugins": [
				"babel-plugin-redux-saga",
				"./babel-plugin-electron-module-resolver"
			]
		}
	}
}
