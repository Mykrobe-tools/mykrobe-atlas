{
  "name": "mykrobe-atlas",
  "productName": "Mykrobe Predictor TB",
  "version": "0.2.0",
  "description": "Mykrobe Atlas UI",
  "private": true,
  "main": "main.js",
  "scripts": {
    "test": "cross-env BABEL_DISABLE_CACHE=1 jest",
    "test:watch": "npm test -- --watch",
    "flow": "flow",
    "lint": "npm-run-all --serial lint:css lint:js",
    "lint:css": "stylelint \"app/**/*.css\"",
    "lint:css:fix": "stylelint --fix \"app/**/*.css\"",
    "lint:js": "eslint app/**/*.js",
    "lint:js:fix": "eslint --fix app/**/*.js",
    "precommit": "npm run lint:css:fix && npm run lint:js:fix",
    "electron-hot-server": "node -r babel-register electron/dev-server.js",
    "electron-build-main": "cross-env NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config electron/webpack.config.electron.js --progress --profile --colors",
    "electron-build-renderer": "cross-env NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config electron/webpack.config.production.js --progress --profile --colors",
    "electron-build": "npm run electron-build-main && npm run electron-build-renderer",
    "electron-start": "cross-env NODE_ENV=production electron ./electron/static/",
    "electron-start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./electron/index.electron",
    "electron-package": "cross-env NODE_ENV=production babel-node ./electron/package.js",
    "electron-package:all": "npm run electron-package -- --all",
    "electron-dev": "concurrently \"npm run electron-hot-server\" \"npm run electron-start-hot\"",
    "electron-dist": "electron-builder --x64",
    "electron-dist:win": "electron-builder -w --x64",
    "electron-dist:all": "electron-builder -mwl --x64",
    "electron-compile": "npm run electron-build-renderer && npm run electron-build-main",
    "postinstall": "electron-builder install-app-deps",
    "set-target": "babel-node scripts/set-target.js",
    "generate-icons": "babel-node scripts/generate-icons.js",
    "web-api-server": "node -r babel-register web/api-dev-server.js",
    "web-hot-server": "node -r babel-register web/dev-server.js",
    "web-dev": "concurrently \"npm run web-hot-server\" \"npm run web-api-server\" \"npm run web-open\"",
    "web-open": "open http://localhost:3000",
    "web-build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config web/webpack.config.production.js --progress --profile --colors",
    "web-build-server": "node -r babel-register web/build/simple-server.js",
    "web-build-simple-server": "concurrently \"npm run web-build-server\" \"npm run web-open\""
  },
  "build": {
    "appId": "com.mykrobe.predictor-tb",
    "mac": {
      "category": "public.app-category.tools",
      "icon": "./electron/resources/icon/predictor-tb/icon.icns"
    },
    "files": [
      "**/*",
      "!**/*.ctx"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "./electron/resources/icon/predictor-tb/icon.ico"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "bam",
        "name": "bam"
      },
      {
        "ext": "gz",
        "name": "gz"
      },
      {
        "ext": "json",
        "name": "json"
      },
      {
        "ext": "fastq",
        "name": "fastq"
      }
    ],
    "directories": {
      "buildResources": "build",
      "output": "electron/dist",
      "app": "electron/static"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "asar": "^0.12.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.16.0",
    "concurrently": "^2.2.0",
    "connect-multiparty": "^2.0.0",
    "cors": "^2.8.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "devtron": "^1.2.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^9.1.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.0",
    "fs-extra": "^0.30.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^1.1.3",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "nock": "^9.0.2",
    "node-libs-browser": "^1.0.0",
    "prettier": "^1.7.4",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "stylelint": "^8.2.0",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "css-modules-require-hook": "^4.0.1",
    "electron-debug": "^1.0.1",
    "font-awesome": "^4.6.3",
    "fuse.js": "^2.6.1",
    "google-maps": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "load-script": "^1.0.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "phylocanvas": "^2.6.1",
    "react": "15.4.2",
    "react-autosuggest": "^8.0.0",
    "react-cookie": "^1.0.5",
    "react-datepicker": "^0.29.0",
    "react-dom": "15.4.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "resumablejs": "23/resumable.js",
    "source-map-support": "^0.4.2",
    "spark-md5": "^3.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "targetName": "predictor-tb",
  "displayVersion": "0.2.0 (beta)",
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/_mocks/jestFileMock.js"
    }
  }
}
