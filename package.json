{
  "name": "mykrobe-atlas",
  "productName": "Mykrobe",
  "version": "0.8.1",
  "description": "Mykrobe",
  "private": true,
  "main": "main.js",
  "scripts": {
    "check:audit": "yarn audit --groups dependencies --level moderate",
    "check:es": "yarn web-build && yarn es-check es5 ./web/build/**/*.js || (echo '\nOne or more build files contains non-es5 code. This is usually caused by a module in the node_modules folder distributing non-transpiled source code. Check recently added or updated modules.' && false)",
    "desktop-build-main": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.electron.js",
    "desktop-build-renderer": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.production.js",
    "desktop-build": "yarn desktop-build-main && yarn desktop-build-renderer",
    "desktop-dev": "cross-env PLATFORM=electron UPGRADE_EXTENSIONS=1 concurrently -k -r \"yarn desktop-hot-server\" \"yarn desktop-start-hot\"",
    "desktop-dist:all": "yarn desktop-dist --all --x64",
    "desktop-dist:win": "yarn desktop-dist --win --x64",
    "desktop-dist": "cross-env DEBUG=mykrobe*,electron* PLATFORM=electron babel-node ./desktop/dist.js",
    "desktop-hot-server": "cross-env PLATFORM=electron NODE_ENV=development webpack-dev-server --hot --port 3000 --inline --history-api-fallback --progress --config ./desktop/webpack.config.development.js",
    "desktop-install-dependencies": "electron-builder install-app-deps",
    "desktop-package:all": "yarn desktop-package --all",
    "desktop-package:win": "yarn desktop-package --win --x64",
    "desktop-package": "cross-env DEBUG=mykrobe*,electron* PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js",
    "desktop-start-hot": "cross-env HOT=1 PLATFORM=electron NODE_ENV=development electron --inspect=5858 --require @babel/register ./desktop/index.desktop",
    "desktop-start": "cross-env PLATFORM=electron NODE_ENV=production electron ./desktop/build/",
    "fetch-predictor-binaries": "cross-env DEBUG=mykrobe* babel-node ./desktop/fetchPredictorBinaries.js",
    "flow": "flow",
    "generate-icons": "cross-env DEBUG=mykrobe* babel-node scripts/generate-icons.js",
    "lint:eslint": "eslint ./",
    "lint:prettier": "prettier -l \"./**/*.{js,json,css,scss}\" || (echo '\nThese files are not formatted correctly. Did you forget to run \"yarn prettier\"?' && false)",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "migrate-css-module-filenames": "cross-env DEBUG=ms:* babel-node scripts/migrate-css-module-filenames.js --src ./app",
    "precommit": "yarn lint && yarn test:fast && yarn check:es && yarn check:audit",
    "prettier": "prettier --write \"./**/*.{js,json,css,scss}\"",
    "storybook": "start-storybook --port 6006",
    "test:debug": "echo \"To attach Chrome DevTools, open chrome://inspect/#devices then click 'Open dedicated DevTools for node'\" && node --inspect node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test -t 'e2e'",
    "test:fast": "cross-env INCLUDE_SLOW_TESTS=false jest --no-cache",
    "test:watch": "yarn test:fast --watch",
    "test": "cross-env INCLUDE_SLOW_TESTS=true DEBUG=mykrobe*,electron* jest --no-cache --bail --runInBand",
    "web-api-server": "babel-node web/api-dev-server.js",
    "web-build-server": "babel-node web/web-server-example.js",
    "web-build": "cross-env NODE_ENV=production webpack --config ./web/webpack.config.production.js",
    "web-dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --port 3000 --inline --history-api-fallback --progress --config ./web/webpack.config.development.js",
    "web-dev": "yarn web-dev-server",
    "web-open": "open http://localhost:3000"
  },
  "build": {
    "appId": "com.mykrobe.desktop",
    "publish": {
      "provider": "github",
      "owner": "Mykrobe-tools",
      "repo": "mykrobe"
    },
    "mac": {
      "minimumSystemVersion": "10.13",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./desktop/resources/plist/entitlements.mac.plist",
      "entitlementsInherit": "./desktop/resources/plist/entitlements.mac.plist",
      "category": "public.app-category.tools",
      "identity": "Make and Ship Limited",
      "icon": "./desktop/resources/icon/desktop/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "sign": false
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "./desktop/resources/icon/desktop/icon.ico"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "bam",
        "name": "bam"
      },
      {
        "ext": "gz",
        "name": "gz"
      },
      {
        "ext": "json",
        "name": "json"
      },
      {
        "ext": "fastq",
        "name": "fastq"
      }
    ],
    "directories": {
      "buildResources": "./desktop/resources/build",
      "output": "./desktop/dist",
      "app": "./desktop/build"
    },
    "afterSign": "./desktop/notarize.js"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-decorators": "^7.8.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.0",
    "@babel/plugin-transform-flow-strip-types": "^7.8.0",
    "@babel/plugin-transform-modules-umd": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-flow": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@babel/runtime": "^7.8.2",
    "@storybook/react": "^5.3.0",
    "autoprefixer": "^9.7.0",
    "axios-curlirize": "^1.3.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "body-parser": "^1.16.0",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "^1.1.0",
    "concurrently": "^5.1.0",
    "connect-multiparty": "^2.1.0",
    "connected-react-router": "^6.3.2",
    "core-js": "^3.6",
    "cors": "^2.8.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.0",
    "debug": "^4.1.1",
    "decompress": "^4.2.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "dotenv": "^8.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-log": "^4.1.1",
    "electron-packager": "^14.2.1",
    "electron-updater": "^4.3.1",
    "enzyme": "^3.2.0",
    "es-check": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "express": "^4.14.0",
    "express-http-proxy": "^1.1.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.123.0",
    "flow-typed": "^3.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^25.5.3",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.0",
    "module": "^1.2.5",
    "nock": "^9.1.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.1",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "redux-saga-testing": "^1.0.5",
    "sass-loader": "^8.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.0",
    "terser-webpack-plugin": "^2.3.0",
    "tmp": "^0.0.33",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.0",
    "webpack-merge": "^4.2.0"
  },
  "dependencies": {
    "@google/markerclustererplus": "^2.1.11",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.1.0",
    "date-fns": "^2.8.1",
    "event-source-polyfill": "^1.0.15",
    "fbjs-scripts": "^0.8.3",
    "font-awesome": "^4.7.0",
    "fs-extra": "^6.0.1",
    "fuse.js": "^2.6.1",
    "google-maps": "^3.3.0",
    "history": "^4.7.2",
    "http-status-codes": "^1.3.0",
    "immer": "^3.1.2",
    "is_js": "^0.9.0",
    "js-file-download": "^0.4.12",
    "keycloak-js": "^10.0.2",
    "load-script": "^1.0.0",
    "lodash": "^4.17.19",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isnil": "^4.0.0",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
