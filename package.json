{
  "name": "mykrobe-atlas",
  "productName": "Mykrobe",
  "version": "0.6.1",
  "description": "Mykrobe",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build-mykrobe-binaries": "babel-node ./desktop/build-mykrobe-binaries.js",
    "check:es": "yarn web-build && yarn es-check es5 ./web/build/**/*.js || (echo '\nOne or more build files contains non-es5 code. This is usually caused by a module in the node_modules folder distributing non-transpiled source code. Check recently added or updated modules.' && false)",
    "desktop-build-main": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.electron.js",
    "desktop-build-renderer": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.production.js",
    "desktop-build": "yarn desktop-build-main && yarn desktop-build-renderer",
    "desktop-dev": "cross-env PLATFORM=electron UPGRADE_EXTENSIONS=1 concurrently \"yarn desktop-hot-server\" \"yarn desktop-start-hot\"",
    "desktop-dist:all": "cross-env PLATFORM=electron babel-node ./desktop/dist.js --all --x64",
    "desktop-dist:win": "cross-env PLATFORM=electron babel-node ./desktop/dist.js --win --x64",
    "desktop-dist": "cross-env DEBUG=electron-builder,electron-builder:* PLATFORM=electron babel-node ./desktop/dist.js",
    "desktop-hot-server": "cross-env PLATFORM=electron NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 3000 --inline --history-api-fallback --progress --config ./desktop/webpack.config.development.js",
    "desktop-install-dependencies": "electron-builder install-app-deps",
    "desktop-package:all": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js --all",
    "desktop-package:win": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js --win --x64",
    "desktop-package": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js",
    "desktop-start-hot": "cross-env HOT=1 PLATFORM=electron NODE_ENV=development electron -r @babel/register -r @babel/polyfill ./desktop/index.desktop",
    "desktop-start": "cross-env PLATFORM=electron NODE_ENV=production electron ./desktop/static/",
    "flow": "flow",
    "generate-icons": "babel-node scripts/generate-icons.js",
    "lint:eslint": "eslint ./",
    "lint:prettier": "prettier -l \"./**/*.{js,json,css,scss}\" || (echo '\nThese files are not formatted correctly. Did you forget to run \"yarn prettier\"?' && false)",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "precommit": "yarn lint && yarn test:fast && yarn check:es",
    "prettier": "prettier --write \"./**/*.{js,json,css,scss}\"",
    "set-target": "babel-node scripts/set-target.js",
    "storybook": "start-storybook --host 0.0.0.0 --port 6006",
    "test:debug": "echo \"To attach Chrome DevTools, open chrome://inspect/#devices then click 'Open dedicated DevTools for node'\" && node --inspect node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test -t 'e2e'",
    "test:fast": "cross-env INCLUDE_SLOW_TESTS=false jest --no-cache",
    "test:watch": "yarn test:fast --watch",
    "test": "cross-env INCLUDE_SLOW_TESTS=true jest --no-cache --bail --runInBand",
    "web-api-server": "babel-node web/api-dev-server.js",
    "web-build-server": "babel-node web/build/simple-server.js",
    "web-build-server-compile": "rm -rf web/dist && babel web/build --out-dir web/dist",
    "web-build-server-start": "node web/dist/simple-server.js",
    "web-build-simple-server": "concurrently \"yarn web-build-server\" \"yarn web-open\"",
    "web-build": "cross-env NODE_ENV=production webpack --config ./web/webpack.config.production.js",
    "web-dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 3000 --inline --history-api-fallback --progress --config ./web/webpack.config.development.js",
    "web-dev": "yarn web-dev-server",
    "web-open": "open http://localhost:3000"
  },
  "build": {
    "appId": "com.mykrobe.desktop",
    "publish": {
      "provider": "github",
      "owner": "Mykrobe-tools",
      "repo": "mykrobe"
    },
    "mac": {
      "category": "public.app-category.tools",
      "icon": "./desktop/resources/icon/desktop/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "./desktop/resources/icon/desktop/icon.ico"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "bam",
        "name": "bam"
      },
      {
        "ext": "gz",
        "name": "gz"
      },
      {
        "ext": "json",
        "name": "json"
      },
      {
        "ext": "fastq",
        "name": "fastq"
      }
    ],
    "directories": {
      "buildResources": "desktop/resources/build",
      "output": "desktop/dist",
      "app": "desktop/static"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-destructuring": "^7.4.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@storybook/react": "^5.0.11",
    "asar": "^0.14.3",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-dotenv": "^1.3.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "body-parser": "^1.16.0",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^2.0.0",
    "concurrently": "^3.5.1",
    "connect-multiparty": "^2.1.0",
    "connected-react-router": "^6.3.2",
    "cors": "^2.8.1",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.0",
    "css-modules-require-hook": "^4.2.2",
    "del": "^3.0.0",
    "devtron": "^1.2.1",
    "electron": "^2.0.2",
    "electron-builder": "^20.28.1",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.1",
    "electron-log": "^2.2.11",
    "electron-packager": "^12.1.0",
    "electron-updater": "^2.17.6",
    "enzyme": "^3.2.0",
    "eslint": "^5.15.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-flowtype": "^3.4.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.0",
    "express": "^4.14.0",
    "express-http-proxy": "^1.1.0",
    "file-loader": "^2.0.0",
    "flow-typed": "^2.3.0",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^4.0.0",
    "jest": "^24.5.0",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.0",
    "minimist": "^1.2.0",
    "module": "^1.2.5",
    "nock": "^9.1.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.0",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "redux-saga-testing": "^1.0.5",
    "sass-loader": "^7.0.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "tmp": "^0.0.33",
    "url-loader": "^1.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "es-check": "^2.1.0",
    "event-source-polyfill": "^0.0.16",
    "fbjs-scripts": "^0.8.3",
    "fs-extra": "^6.0.1",
    "fuse.js": "^2.6.1",
    "global": "^4.3.2",
    "google-maps": "^3.2.1",
    "history": "^4.7.2",
    "is_js": "^0.9.0",
    "jwt-decode": "^2.2.0",
    "load-script": "^1.0.0",
    "lodash": "^4.15.0",
    "makeandship-js-common": "git+ssh://git@bitbucket.org:makeandship/makeandship-js-common#v0.0.89",
    "moment": "^2.14.1",
    "mykrobe-atlas-jsonschema": "git+ssh://git@github.com/Mykrobe-tools/mykrobe-atlas-jsonschema#v0.0.1",
    "parse-filepath": "^1.0.2",
    "phylocanvas": "^2.6.1",
    "pluralize": "^7.0.0",
    "react": "^16.8.0",
    "react-autosuggest": "^9.3.2",
    "react-datepicker": "^1.8.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.0",
    "react-dropzone": "^4.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^2.4.0",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.1.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "resumablejs": "23/resumable.js",
    "spark-md5": "^3.0.0",
    "url-join": "^4.0.0",
    "url-search-params-polyfill": "^5.0.0",
    "whatwg-fetch": "^2.0.4",
    "zxcvbn": "^4.4.2"
  },
  "devEngines": {
    "node": "6.x"
  },
  "targetName": "desktop",
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "/node_modules/(?!(makeandship-js-common|makeandship-api-common|mykrobe-atlas-jsonschema|swagger-client)).+\\.js$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/desktop/static",
      "<rootDir>/desktop/dist",
      "<rootDir>/desktop/release"
    ],
    "globals": {
      "IS_ELECTRON": false
    }
  }
}
