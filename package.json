{
  "name": "mykrobe-atlas",
  "productName": "Mykrobe Atlas TB",
  "version": "0.2.0",
  "description": "Mykrobe Atlas UI",
  "private": true,
  "main": "main.js",
  "scripts": {
    "test:fast": "jest",
    "test": "cross-env INCLUDE_SLOW_TESTS=true jest",
    "test:e2e": "cross-env INCLUDE_SLOW_TESTS=true jest -t 'e2e'",
    "test:watch": "yarn test:fast --watch",
    "flow": "flow",
    "lint:css": "stylelint \"app/**/*.css\"",
    "lint:css:fix": "stylelint --fix \"app/**/*.css\"",
    "lint:js": "eslint app *.js",
    "lint:js:fix": "eslint --fix app *.js",
    "precommit": "yarn lint:css:fix && yarn lint:js:fix && yarn test:fast",
    "desktop-hot-server": "cross-env PLATFORM=electron babel-node desktop/dev-server.js",
    "desktop-build-main": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.electron.js --progress --profile --colors",
    "desktop-build-renderer": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config desktop/webpack.config.production.js --progress --profile --colors",
    "desktop-build": "yarn desktop-build-main && yarn desktop-build-renderer",
    "desktop-start": "cross-env PLATFORM=electron NODE_ENV=production electron ./desktop/static/",
    "desktop-start-hot": "cross-env HOT=1 PLATFORM=electron NODE_ENV=development electron -r babel-register -r babel-polyfill ./desktop/index.desktop",
    "desktop-package": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js",
    "desktop-package:win": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js --win --x64",
    "desktop-package:all": "cross-env PLATFORM=electron NODE_ENV=production babel-node ./desktop/package.js --all",
    "desktop-dev": "cross-env PLATFORM=electron concurrently \"yarn desktop-hot-server\" \"yarn desktop-start-hot\"",
    "desktop-dist": "cross-env DEBUG=electron-builder,electron-builder:* PLATFORM=electron babel-node ./desktop/dist.js",
    "desktop-dist:win": "cross-env PLATFORM=electron babel-node ./desktop/dist.js --win --x64",
    "desktop-dist:all": "cross-env PLATFORM=electron babel-node ./desktop/dist.js --all --x64",
    "build-predictor-binaries": "babel-node ./desktop/build-predictor-binaries.js",
    "postinstall": "electron-builder install-app-deps",
    "set-target": "babel-node scripts/set-target.js",
    "generate-icons": "babel-node scripts/generate-icons.js",
    "web-api-server": "babel-node web/api-dev-server.js",
    "web-hot-server": "babel-node web/dev-server.js",
    "web-dev": "concurrently \"yarn web-hot-server\" \"yarn web-api-server\" \"yarn web-open\"",
    "web-open": "open http://localhost:3000",
    "web-build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config web/webpack.config.production.js --progress --profile --colors",
    "web-build-server": "babel-node web/build/simple-server.js",
    "web-build-simple-server": "concurrently \"yarn web-build-server\" \"yarn web-open\""
  },
  "build": {
    "appId": "com.mykrobe.atlas-tb",
    "publish": {
      "provider": "github",
      "owner": "iqbal-lab",
      "repo": "mykrobe-atlas"
    },
    "mac": {
      "category": "public.app-category.tools",
      "icon": "./desktop/resources/icon/atlas-tb/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "./desktop/resources/icon/atlas-tb/icon.ico"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "bam",
        "name": "bam"
      },
      {
        "ext": "gz",
        "name": "gz"
      },
      {
        "ext": "json",
        "name": "json"
      },
      {
        "ext": "fastq",
        "name": "fastq"
      }
    ],
    "directories": {
      "buildResources": "desktop/resources/build",
      "output": "desktop/dist",
      "app": "desktop/static"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "asar": "^0.12.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.16.0",
    "concurrently": "^3.5.1",
    "connect-multiparty": "^2.1.0",
    "cors": "^2.8.1",
    "cross-env": "^5.1.1",
    "del": "^3.0.0",
    "devtron": "^1.2.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.49.2",
    "electron-log": "^2.2.11",
    "electron-packager": "^10.1.0",
    "electron-updater": "^2.17.6",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "express-http-proxy": "^1.1.0",
    "fbjs-scripts": "^0.7.1",
    "flow-bin": "^0.59.0",
    "fs-extra": "^4.0.2",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^4.0.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "module": "^1.2.5",
    "nock": "^9.1.0",
    "prettier": "^1.9.2",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "selenium-webdriver": "^3.6.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.13.1",
    "stylelint": "^8.2.0",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "url-search-params": "^0.10.0",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-jest": "^22.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-typed": "^2.3.0",
    "font-awesome": "^4.6.3",
    "fuse.js": "^2.6.1",
    "google-maps": "^3.2.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.0.0",
    "jwt-decode": "^2.2.0",
    "load-script": "^1.0.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "parse-filepath": "^1.0.2",
    "phylocanvas": "^2.6.1",
    "postcss": "^6.0.14",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.2",
    "react-cookie": "^1.0.5",
    "react-datepicker": "^0.62.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.5.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "resumablejs": "23/resumable.js",
    "source-map-support": "^0.4.2",
    "spark-md5": "^3.0.0",
    "tmp": "^0.0.33",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1"
  },
  "devEngines": {
    "node": "6.x"
  },
  "targetName": "atlas-tb",
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/_mocks/jestFileMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/desktop/static",
      "<rootDir>/desktop/dist",
      "<rootDir>/desktop/release"
    ],
    "globals": {
      "IS_ELECTRON": false
    }
  }
}